[masters]
master ansible_host=swarm-master

[masters:vars]
ansible_user=user
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=-/.ssh/id_rsa

[workers]
worker-1 ansible_host=project2-vm1
worker-2 ansible_host=project2-vm2

[workers:vars]
ansible_user=user
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=-/.ssh/id_rsa
